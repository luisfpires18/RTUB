{
  "UseSqlServer": false,
  "ImageCaching": {
    "CacheDurationInSeconds": 86400
  },
  "IDrive": {
    "AccessKey": "your-idrive-readonly-access-key",
    "SecretKey": "your-idrive-readonly-secret-key",
    "WriteAccessKey": "your-idrive-write-access-key",
    "WriteSecretKey": "your-idrive-write-secret-key",
    "Endpoint": "your-idrive-endpoint.com",
    "Bucket": "your-bucket-name"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}