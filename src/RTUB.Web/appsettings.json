{
  "UseSqlServer": false,
  "ImageCaching": {
    "CacheDurationInSeconds": 86400
  },
  "Cloudflare": {
    "R2": {
      "Bucket": "rtub"
    }
  },
  "Ranking": {
    "Enabled": true,
    "XpPerRehearsal": 10,
    "XpPerEvent": 20,
    "Levels": [
      { "Level": 1, "Name": "Lei Seca", "XpThreshold": 0 },
      { "Level": 2, "Name": "Vai mais um copo", "XpThreshold": 100 },
      { "Level": 3, "Name": "Já vai alto", "XpThreshold": 250 },
      { "Level": 4, "Name": "Tá na brasa", "XpThreshold": 500 },
      { "Level": 5, "Name": "Veterano das Cervejas", "XpThreshold": 1000 }
    ]
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}